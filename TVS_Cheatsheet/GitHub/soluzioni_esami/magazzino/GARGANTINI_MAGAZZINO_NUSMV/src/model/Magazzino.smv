MODULE main
VAR
  magazzino0: prodotto;
  magazzino1: prodotto;
  magazzino2: prodotto;
  magazzino3: prodotto;
  magazzino4: prodotto;
SPEC
  AG (magazzino0.quantity <=100)

MODULE prodotto
VAR
   quantity : 0..100;
   add: 0..10;
ASSIGN   
   init(quantity) := 0;
   next(quantity) := case
  	quantity + add <= 100 : quantity + add;
  	TRUE:  quantity;
  esac;
SPEC
  AG (quantity <=100)
  
  
